{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Pivotal": "Information",
      "Steeltoe": "Information",
      "ActuatorsDemo": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information",
        "Pivotal": "Information",
        "Steeltoe": "Information",
        "ActuatorsDemo": "Information"
      }
    }
  },

  "info": {
    "appsetting1": "appsetting1",
    "appsetting2": "appsetting2",
    "appsetting3": "appsetting3",
    "appsetting4": "appsetting4"
  },

  "spring": {
    "application": {
      "name": "fortuneServiceUnity"
    },

    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false
      },
      "application": {
        "name": "fortuneServiceUnity"
      }
    }
  },

  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldFetchRegistry": false,
      "validate_certificates": false
    }
  },

  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },

  "sqlserver": {
    "credentials": {
      "connectionString": "Server=(localdb)\\mssqllocaldb;database=Steeltoe;Trusted_Connection=True;"
    }
  }

}

