{
  "Logging": {
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information",
      "Pivotal": "Information",
      "Steeltoe": "Information",
      "ActuatorsDemo": "Information"
    },
    "Console": {
      "IncludeScopes": true,
      "LogLevel": {
        "Default": "Debug",
        "System": "Information",
        "Microsoft": "Information",
        "Pivotal": "Information",
        "Steeltoe": "Information",
        "ActuatorsDemo": "Information"
      }
    }
  },

  "info": {
    "appsetting1": "appsetting1",
    "appsetting2": "appsetting2",
    "appsetting3": "appsetting3",
    "appsetting4": "appsetting4"
  },

  "RANDOM_FORTUNE_URL": "http://fortuneServiceUnity/api/fortunes/random",

  "spring": {
    "application": {
      "name": "fortuneUiUnity"
    },

    "cloud": {
      "config": {
        "uri": "http://localhost:8888",
        "validate_certificates": false
      },
      "application": {
        "name": "fortuneUiUnity"
      }
    }
  },

  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/",
      "shouldRegisterWithEureka": false,
      "validate_certificates": false
    }
  },

  "management": {
    "endpoints": {
      "path": "/cloudfoundryapplication",
      "cloudfoundry": {
        "validateCertificates": false
      }
    }
  },

  "redis": {
    "client": {
      "connectRetry": 3
    }
  },

  "hystrix": {
    "stream": {
      "validate_certificates": false
    },
    "command": {
      "FortuneService": {
        "threadPoolKeyOverride": "FortuneServiceTPool"
      }
    }
  }
}




